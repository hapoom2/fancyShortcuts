{
  "manifest_version": 3,
  "name": "Fancy Shortcuts",
  "version": "1.0.0",
  "description": "새 탭 시작페이지에 숏컷과 배경이미지를 설정합니다.",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": ["storage"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
